CREATE TABLE "LICENCIA" (
"LIC_ID" INTEGER NOT NULL primary key
        GENERATED ALWAYS AS IDENTITY
		(START WITH 1, INCREMENT BY 1),
"LIC_CLIENT" INTEGER NOT NULL,
"LIC_TIPO" INTEGER NOT NULL,
"LIC_RENOVACION" DATE DEFAULT NULL,
constraint FK_CLIENTE foreign key (LIC_CLIENT) references CLIENTES(CLIENT_ID),
constraint FK_TIPO foreign key (LIC_TIPO) references CARNET(CARNET_ID)
);

INSERT INTO LICENCIA(LIC_CLIENT,LIC_TIPO, LIC_RENOVACION) VALUES
	(1,5,NULL),
	(2,5,NULL),
	(3,5,NULL);
	