CREATE TABLE "CARNET" (
"CARNET_ID" INTEGER NOT NULL primary key
        GENERATED ALWAYS AS IDENTITY
		(START WITH 1, INCREMENT BY 1), 
"CARNET_TIPO" VARCHAR(3) NOT NULL 
);

INSERT INTO CARNET(CARNET_TIPO) VALUES 
	('AM'),('A1'),('A2'),('A'),('B'),('C1'),('C'),('D1'),('D'),('BE'),('C1E'),('CE'),('D1E'),('DE'),('btp');
	